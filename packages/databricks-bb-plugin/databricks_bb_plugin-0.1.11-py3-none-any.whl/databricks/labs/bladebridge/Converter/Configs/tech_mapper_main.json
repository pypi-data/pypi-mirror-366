// this is the main mapping json file to map source technologies to appropriate json files
// the converters will use this file to map the command line arg to the config file
{
  "INFAPC" : {
    "source-type": "INFA",
    "targets":  {
      "SPARKSQL" : "final_infapc2sparksql.json",
      "PYSPARK" : "final_infapc2pyspark.json"
    }
  },
   "INFACLOUD" : {
    "source-type": "INFA",
    "targets":  {
      "PYSPARK" : "infacloud2pyspark.json"
    }
  },
  "DATASTAGE" : {
    "source-type": "DATASTAGE",
    "targets" : {
      "SPARKSQL" : "final_datastage2sparksql.json",
      "PYSPARK" : "final_datastage2pyspark.json"
    }
  },
  "SYNAPSE" : {
    "source-type": "SQL",
    "targets": {
      "SQL": "base_synapse2databricks_sql.json"
    }
  },
  "ORACLE" : {
    "source-type": "SQL",
    "targets": {
      "SQL": "base_oracle2databricks_sql.json"
    }
  },
  "MSSQL" : {
    "source-type": "SQL",
    "targets": {
      "SQL": "base_sqlserver2databricks_sql.json"
    }
  },
  "NETEZZA" : {
    "source-type": "SQL",
    "targets": {
      "SQL": "netezza2sparksql.json"
    }
  },
  "TERADATA" : {
    "source-type": "SQL",
    "targets": {
      "SQL": "teradata2databricks_sql_main.json"
    }
  },
  "DEFAULT" : {
    "source-type": "SQL",
    "targets": {
      "SQL": "base_sqlserver2databricks_sql.json" // this is random, we can choose another default mapper
    }
  }
}
