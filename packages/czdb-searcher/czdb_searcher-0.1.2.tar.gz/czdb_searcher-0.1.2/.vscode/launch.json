    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}",
            "python": "${workspaceFolder}/venv/bin/python"
        },
        {
            "name": "Run main.py with CZDB",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/venv/bin/python",
            "args": [
                "/Users/liucong/Downloads/ipv6_score_type_data.czdb",
                "BTREE",
                "3fEhuZUEvDzRjKv9qvAzTQ=="
            ]
        }
    ]
