{
  "name": "baas-sms-mcp",
  "version": "1.0.0",
  "description": "A Model Context Protocol server for SMS and MMS messaging services",
  "main": "index.js",clear
  "bin": {
    "baas-sms-mcp": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "python -m pip install -r requirements.txt",
    "release:patch": "npm version patch && git push origin master --tags && npm publish",
    "release:minor": "npm version minor && git push origin master --tags && npm publish",
    "release:major": "npm version major && git push origin master --tags && npm publish",
    "build": "npm pack",
    "clean": "rm -f *.tgz"
  },
  "keywords": [
    "mcp",
    "sms",
    "mms",
    "messaging",
    "model-context-protocol",
    "baas"
  ],
  "author": "mBaaS Team <jjunmo@mbaas.kr>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/baas-sms-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/baas-sms-mcp/issues"
  },
  "homepage": "https://github.com/your-org/baas-sms-mcp#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "files": [
    "index.js",
    "requirements.txt",
    "baas_sms_mcp/",
    "README.md",
    "README.ko.md",
    "LICENSE"
  ],
  "preferGlobal": true
}
